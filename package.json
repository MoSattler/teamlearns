{
    "name": "@teamlearns/directory",
    "version": "1.0.0",
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "serve": "turbo run serve",
        "dev": "turbo run dev --parallel",
        "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
